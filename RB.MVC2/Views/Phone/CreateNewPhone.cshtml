@using RB.DAL.Models
@model Phones
<li>
    <div>
        @Html.DisplayFor(modelItem => @Model.PhoneNumber)
        @Html.HiddenFor(modelItem => @Model.CreatedOn)
        @Html.HiddenFor(modelItem => @Model.PhoneId)
        @Html.HiddenFor(modelItem => @Model.CompanyId)
        <a asp-action="Edit" asp-route-id="@Model.PhoneId" class="btn btn-info editPhone" data-id="@Model.PhoneId">Edit</a>
        <a asp-action="Delete" asp-route-id="@Model.PhoneId" class="btn btn-danger delPhone" data-id="@Model.PhoneId">Delete</a>
    </div>
</li>